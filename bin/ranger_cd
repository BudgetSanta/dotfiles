#!/bin/bash

# Ranger exit into current Dir
tempfile=$(mktemp /tmp/${tempfoo}.XXXXXX)
/home/linuxbrew/.linuxbrew/bin/ranger --choosedir="$tempfile" "${@:-$(pwd)}" < $TTY
test -f "$tempfile" &&
if [ "$(cat -- "$tempfile")" != "$(echo -n `pwd`)" ]; then
    cd -- "$(cat "$tempfile")"
fi
rm -f -- "$tempfile"
